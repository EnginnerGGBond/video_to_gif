{
  "name": "rswb",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open -c rsbuild.dev.config.ts",
    "build": "rsbuild build -c rsbuild.prod.config.ts",
    "preview": "rsbuild preview",
    "prepare": "husky",
    "commit": "commit",
    "changelog": "git-cliff",
    "release": "git-cliff --tag && git push --follow-tags && npm run build"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/prompt-cli": "^19.2.0",
    "@rsbuild/core": "^0.4.15",
    "@rsbuild/plugin-check-syntax": "^0.4.15",
    "@rsbuild/plugin-image-compress": "^0.4.15",
    "@rsbuild/plugin-react": "^0.4.15",
    "@rsbuild/plugin-type-check": "^0.4.15",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "commitlint": "^19.2.0",
    "git-cliff": "^2.1.2",
    "husky": "9.0.11",
    "lint-staged": "^15.2.2",
    "oxlint": "^0.2.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
